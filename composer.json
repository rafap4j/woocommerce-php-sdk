{
    "name": "rafap4j/woocommerce-php-sdk",
    "description": "WooCommerce PHP SDK.",
    "keywords": [
        "rafap4j",
        "woocommerce-php-sdk"
    ],
    "homepage": "https://github.com/rafap4j/woocommerce-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Augusto",
            "email": "rafap4j@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rafap4j\\WoocommercePhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafap4j\\WoocommercePhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
